        <hibernate-ehcache.version>6.0.0.Alpha7</hibernate-ehcache.version>
        <hibernate.version>6.1.6.Final</hibernate.version>
        <hikari.version>5.0.1</hikari.version>
        <ehcache.version>3.10.8</ehcache.version>
        <jaxb.version>2.3.1</jaxb.version>
        <jackson.version>2.14.1</jackson.version>
        <jakarta.version>10.0.0</jakarta.version>
        <javafaker.version>1.0.2</javafaker.version>
        <jsonwebtoken.version>0.11.5</jsonwebtoken.version>
        <log4j.version>2.19.0</log4j.version>
        <lombok-mapstruct-binding.version>0.2.0</lombok-mapstruct-binding.version>
        <lombok.version>1.18.24</lombok.version>
        <mapstruct.version>1.5.3.Final</mapstruct.version>
        <maven-compiler.version>3.10.1</maven-compiler.version>
        <maven-war.version>3.3.2</maven-war.version>
        <maven.compiler.source>17</maven.compiler.source>
        <maven.compiler.target>17</maven.compiler.target>
        <pdfbox.version>2.0.27</pdfbox.version>
        <postgresql.version>42.5.1</postgresql.version>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <slf4j.version>2.0.6</slf4j.version>
        <snakeyaml.version>1.33</snakeyaml.version>
        <spring-data.version>2022.0.0</spring-data.version>
        <spring-framework.version>6.0.3</spring-framework.version>
        <spring-security.version>6.0.1</spring-security.version>
        <suppress-warnings.version>4.0.0-20160619211629</suppress-warnings.version>
        <thumbnailator.version>0.4.19</thumbnailator.version>
        <thymeleaf.version>3.1.1.RELEASE</thymeleaf.version>
        <versions-plugin.version>2.13.0</versions-plugin.version>
        <yaml-properties.version>1.1.3</yaml-properties.version>
        <yaml-resource-bundle.version>0.1</yaml-resource-bundle.version>
    </properties>

    <dependencies>
        <dependency>
            <artifactId>spring-webmvc</artifactId>
